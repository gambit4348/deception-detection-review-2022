@article{ WOS:000663708000022,
Author = {Khan, Wasiq and Crockett, Keeley and O'Shea, James and Hussain, Abir and
   Khan, Bilal M.},
Title = {Deception in the eyes of deceiver: A computer vision and machine
   learning based automated deception detection},
