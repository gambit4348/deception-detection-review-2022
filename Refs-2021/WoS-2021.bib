@inproceedings{ WOS:000776504300014,
Author = {Pasquali, Dario and Gaggero, Davide and Volpe, Gualtiero and Rea,
   Francesco and Sciutti, Alessandra},
Editor = {Li, H and Ge, SS and Wu, Y and Wykowska, A and He, H and Liu, X and Li, D and PerezOsorio, J},
Title = {Human vs Robot Lie Detector: Better Working as a Team?},
Booktitle = {SOCIAL ROBOTICS, ICSR 2021},
