@article{ ISI:000496334800014,
Author = {Saquete, Estela and Tomas, David and Moreda, Paloma and Martinez-Barco,
   Patricio and Palomar, Manuel},
Title = {{Fighting post-truth using natural language processing: A review and open
   challenges}},
