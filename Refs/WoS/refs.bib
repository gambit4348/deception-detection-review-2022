Title = {{The Fuzzy Emotion Recognition Framework Using Semantic-Linguistic Facial
   Features}},
Booktitle = {{PROCEEDINGS OF 2019 IEEE R10 HUMANITARIAN TECHNOLOGY CONFERENCE (IEEE
   R10 HTC 2019)}},
Series = {{IEEE Region 10 Humanitarian Technology Conference}},
