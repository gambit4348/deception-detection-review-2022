Title = {{A Comparative Study of Classifier Performance on Spatial and Temporal
   Features of Handwritten Behavioural Data}},
Booktitle = {{INTELLIGENT HUMAN COMPUTER INTERACTION, IHCI 2016}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2017}},
Volume = {{10127}},
